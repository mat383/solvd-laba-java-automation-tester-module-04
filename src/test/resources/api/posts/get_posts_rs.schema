{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "com.solvd.laba.testing.api.posts",
  "title": "posts",
  "description": "Array of all posts",
  "type": "array",
  "minItems": 0,
  "uniqueItems": true,
  "items": {
    "type": "object",
    "properties": {
      "userId": {
        "type": "integer"
      },
      "id": {
        "type": "integer"
      },
      "title": {
        "type": "string"
      },
      "body": {
        "type": "string"
      }
    },
    "required": [
      "id"
    ]
  }
}